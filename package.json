    "apollo-link-http": "^1.5.16",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.3.0",
    "next-seo": "^4.4.0",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "tailwindcss": "^1.2.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.13.0",
    "@graphql-codegen/typescript": "^1.13.0",
    "@types/node": "^13.9.0",
    "@types/react": "^16.9.23",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
