{
  "name": "webapp",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/getbard/webapp",
  "author": "Anthony Morris <anthony@amorrissound.com>",
  "scripts": {
    "predev": "yarn schema && yarn generate",
    "dev": "next dev",
    "prebuild": "yarn generate",
